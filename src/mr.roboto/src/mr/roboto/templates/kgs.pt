[buildout]
extends =
    core.cfg
    https://raw.github.com/plone/buildout.jenkins/master/jenkins.cfg
always-accept-server-certificate = true
parts += code-analysis

[code-analysis]
recipe = plone.recipe.codeanalysis
directory = ${directory_of_package}
jenkins = True
flake8 = True
jshint = True
csslint = True


${structure: sources}