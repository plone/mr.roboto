[flake8]
ignore =
    # black takes care of line length
    E501,
    # black takes care of where to break lines
    W503,
    # black takes care of blank lines
    D202,
    # black takes care of spaces around colon
    E203,

per-file-ignores =
    src/mr.roboto/src/mr/roboto/subscribers/__init__.py: F401
